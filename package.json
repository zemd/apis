{
  "name": "apis",
  "private": true,
  "scripts": {
    "prebuild": "turbo telemetry disable",
    "build": "turbo build",
    "dev": "turbo dev",
    "generate": "turbo generate-api",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "pnpm run build && changeset publish",
    "changeset": "changeset",
    "test": "turbo test",
    "test-http-client": "turbo test --filter @zemd/http-client",
    "prepare": "husky",
    "lint-publint": "pnpm --filter \"./packages/*\" --filter \"./apis/*\" --no-bail run lint-publint"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@zemd/eslint-ts": "^1.0.34",
    "@zemd/tsconfig": "^1.5.0",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "publint": "^0.3.14",
    "turbo": "^2.5.8"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34"
}
