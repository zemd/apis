{
  "name": "apis",
  "private": true,
  "scripts": {
    "prebuild": "turbo telemetry disable",
    "build": "turbo build",
    "dev": "turbo dev",
    "generate": "turbo generate-api",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "pnpm run build && changeset publish",
    "changeset": "changeset",
    "test": "turbo test",
    "test-http-client": "turbo test --filter @zemd/http-client",
    "prepare": "husky",
    "lint-publint": "pnpm --filter \"./packages/*\" --filter \"./apis/*\" --no-bail run lint-publint"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@zemd/eslint-ts": "^1.0.34",
    "@zemd/tsconfig": "^1.5.0",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "publint": "^0.3.13",
    "turbo": "^2.5.8"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
