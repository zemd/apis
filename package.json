{
  "name": "apis",
  "private": true,
  "scripts": {
    "prebuild": "turbo telemetry disable",
    "build": "turbo build",
    "dev": "turbo dev",
    "generate": "turbo generate-api",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "pnpm run build && changeset publish",
    "changeset": "changeset",
    "test": "turbo test",
    "test-http-client": "turbo test --filter @zemd/http-client",
    "prepare": "husky",
    "lint-publint": "pnpm --filter \"./packages/*\" --filter \"./apis/*\" --no-bail run lint-publint"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@zemd/eslint-ts": "^2.0.1",
    "@zemd/tsconfig": "^1.5.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "publint": "^0.3.17",
    "turbo": "^2.7.6"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
